   "source": [
    "import torch\n",
    "\n",
    "def custom_mse_loss(predictions, targets):\n",
    "    return torch.mean((predictions - targets) ** 2)\n",
    "\n",
    "# Usage\n",
    "pred = torch.tensor([1.5, 2.0])\n",
    "true = torch.tensor([1.0, 2.0])\n",
    "loss = custom_mse_loss(pred, true)\n",
    "print(\"Custom MSE Loss:\", loss.item())\n"
   ]
  }
